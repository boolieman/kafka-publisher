{
  "name": "kafka-publisher-ss",
  "version": "0.3.4",
  "description": "kafka message publisher, fallback appending messages to a file while kafka unavailable",
  "main": "kafka-publisher.js",
  "keywords": [
    "kafka",
    "publisher",
    "fallback"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "watchInteg": "NODE_ENV=DEBUG ava --fail-fast -v **/__integ-tests__/**/*test*.js --watch",
    "watchUnit": "NODE_ENV=DEBUG ava --fail-fast -v **/__tests__/**/*test*.js --watch",
    "coverage": "nyc ava",
    "coverageDetail": "nyc --reporter=html --reporter=text ava",
    "ava": "ava",
    "test": "npm run ava",
    "build": "babel lib --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "start": "nodemon --exec babel-node lib/logging.js",
    "lint": "eslint . --ext=js --ignore-pattern '**/__tests__' --ignore-pattern '**/__integ-tests__' --ignore-pattern '**/public/api-docs' --ignore-path .gitignore --fix",
    "watchLint": "esw . --ext=js --ignore-pattern '**/__tests__' --ignore-pattern '**/__integ-tests__' --ignore-pattern '**/public/api-docs' --ignore-path .gitignore --fix --watch"
  },
  "esm": {
    "force": true,
    "mode": "auto"
  },
  "author": "Troy Collinsworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:tcollinsworth/kafka-publisher.git"
  },
  "devDependencies": {
    "async-wait-until": "^1.2.4",
    "ava": "^0.16.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-watch": "^4.0.2",
    "ignore-styles": "^5.0.1",
    "nyc": "^11.2.1",
    "sinon": "^2.3.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "delay": "^4.1.0",
    "esm": "^3.0.84",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "moment": "^2.24.0",
    "node-rdkafka": "^2.5.1",
    "pino": "^5.11.1",
    "pino-pretty": "^2.4.2",
    "promise-retry": "^1.1.1",
    "promise-timeout": "^1.3.0",
    "stdout-stream": "^1.4.0",
    "uuid": "^3.3.2"
  },
  "ava": {
    "files": [
      "**/__tests__/**/*test*.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "ignore-styles",
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
